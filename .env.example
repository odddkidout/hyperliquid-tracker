# Hyperliquid API Configuration
HYPERLIQUID_API_KEY=your_api_key_here
HYPERLIQUID_SECRET=your_secret_here
HYPERLIQUID_WALLET_ADDRESS=your_wallet_address_here

# Addresses to Track (comma-separated, leave empty to use default list)
# Add Hyperliquid wallet addresses you want to track, separated by commas
# Example: 0x123...,0x456...,0x789...
TRACKED_ADDRESSES=

# Trading Configuration
COPY_TRADE_ENABLED=false
POSITION_SIZE_MULTIPLIER=0.1
MAX_POSITION_SIZE=1000
MIN_WIN_RATE=0.6
MIN_TRADES=50

# Database
DATABASE_URL=sqlite:///hyperliquid_tracker.db
